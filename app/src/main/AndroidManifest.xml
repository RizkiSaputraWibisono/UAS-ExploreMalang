<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="polinema.ac.id.ExploreMalang">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo3"
        android:label="Explore Malang!"
        android:roundIcon="@drawable/logo3"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="polinema.ac.id.ExploreMalang.UAS.activities.WelcomeActivity"></activity>
        <activity android:name="polinema.ac.id.ExploreMalang.UAS.activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="polinema.ac.id.ExploreMalang.UAS.activities.MainActivity"></activity>
    </application>

</manifest>